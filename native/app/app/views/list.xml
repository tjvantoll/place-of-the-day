<Page navigatedTo="load">
	<GridPanel>
		<ListView items="{{ places }}" itemTap="details">
			<ListView.itemTemplate>
				<StackPanel>
					<Label text="{{ Name + ', ' + Country }}" />
					<Label text="{{ PushDate }}" />
				</StackPanel>
			</ListView.itemTemplate>
		</ListView>
	</GridPanel>
</Page>